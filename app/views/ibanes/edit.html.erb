<%= form_for @ibane do |f| %>
    <div class="field">
    <%= f.label :曲名 %>
    <%= f.text_field :name, :size => 30 %>
  </div>
  <div class="field">
  <%= f.label :アーティスト名 %>
  <%= f.text_field :artist, :size => 30 %>
</div>
<div class="field">
<%= f.label :YouTube音楽サービスURL %>
<%= f.text_field :link, :size => 30 %>
</div>
<div class="field">
    <%= f.label :レビュー  %>
    <%= f.text_area :about, :size => "30x2" %>
  </div>

  <%= f.submit "編集する" %>
<% end %>